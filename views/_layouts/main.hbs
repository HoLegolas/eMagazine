<body>
	<header id="header">
		<!-- Top Header -->
		<div id="top-header">
			<div class="container">
				<div class="header-links">
					<ul>
						<li><a href="/about">About us</a></li>
						<li><a href="/contact">Contact</a></li>

						<li><a href="/privacy">Privacy</a></li>
						<li><form id="frmSignout" action="/account/logout" method="POST"></form></li>
						{{#if isAuthenticated}}
						<li class="nav-item dropdown">
							<a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button"
								data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
								Hi, <b>{{authUser.username}}</b>
							</a>
							<div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown">
								<a class="dropdown-item" href="/account/profile">
									<i class="fa fa-user" aria-hidden="true"></i>
									Profile
								</a>
								<a class="dropdown-item" href="/account/changepassword">
									<i class="fa fa-edit" aria-hidden="true"></i>
									Password
								</a>
								<a class="dropdown-item" href="javascript: $('#frmSignout').submit();">
									<i class="fa fa-sign-out" aria-hidden="true"></i>
									Sign Out
								</a>
							</div>
						</li>
						{{else}}
						<li class="nav-item">
							<li><a href="/account/signup"><i class="fa fa-user-plus"></i> SignUp</a></li>
						</li>
						<li class="nav-item">
							<li><a href="/account/login"><i class="fa fa-sign-in"></i> LogIn</a></li>
						</li>
						{{/if}}
					</ul>
				</div>
				<div class="header-social">
					<ul>
						<li><a href="#"><i class="fa fa-facebook"></i></a></li>
						<li><a href="#"><i class="fa fa-twitter"></i></a></li>
						<li><a href="#"><i class="fa fa-google-plus"></i></a></li>
						<li><a href="#"><i class="fa fa-instagram"></i></a></li>
						<li><a href="#"><i class="fa fa-youtube"></i></a></li>
					</ul>
				</div>
			</div>
		</div>
		<!-- /Top Header -->

		<!-- Center Header -->
		<div id="center-header">
			<div class="container">
				<div class="header-logo">
					<a href="/home" class="logo"><img src="/img/th16.png" alt="" width="182px" height="274px"></a>
				</div>

			</div>
		</div>
		<!-- /Center Header -->

		<!-- Nav Header -->
		<div id="nav-header">
			<div class="container">
				<nav id="main-nav">
					<div class="nav-logo">
						<a href="/home" class="logo"><img src="/img/logo-alt.png" alt=""></a>
					</div>
					<ul class="main-nav nav navbar-nav">
						{{#each lcCategories}}
						{{#if subcate1}}
						<li class="{{#if active}}active{{/if}}">
							<a id="dLabel" data-target="#" href={{link}} data-toggle="dropdown" role="button" aria-haspopup="true"
								aria-expanded="false">
								{{name}}
								<span class="fa fa-caret-down"></span>
							</a>

							
							<ul class="dropdown-menu" aria-labelledby="dLabel">
								{{#if subcate1}}
								<li class="dropdown-li active"><a href={{link1}}>{{subcate1}}</a></li>
								{{/if}}
								{{#if subcate2}}
								<li class="dropdown-li"><a href={{link2}}>{{subcate2}}</a></li>
								{{/if}}
								{{#if subcate3}}
								<li class="dropdown-li"><a href={{link3}}>{{subcate3}}</a></li>
								{{/if}}
								{{#if subcate4}}
								<li class="dropdown-li"><a href={{link4}}>{{subcate4}}</a></li>
								{{/if}}

								{{#if subcate1}}
								<li></li>
								{{else}}
								<li class="dropdown-li"><a href={{link}}>{{name}}</a></li>
								{{/if}}
								<!--<li class="dropdown-li"><a href="nongsan.html">Nông sản</a></li>
								<li class="dropdown-li"><a href="haisan.html">Hải sản</a></li>-->
							</ul>
						</li>
						{{else}}
							<li><a href={{link}}>{{name}}</a></li>
						{{/if}}
						{{/each}}
